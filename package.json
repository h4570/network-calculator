{
  "name": "network-calculator",
  "version": "0.0.0",
  "description": "Given an IPV4 address and subnet mask, compute the broadcast, network and first/last host address",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/hastoklai/network-calculator.git"
  },
  "bugs": {
    "url": "https://github.com/hastoklai/network-calculator/issues"
  },
  "homepage": "https://github.com/hastoklai/network-calculator",
  "scripts": {
    "test": "jest",
    "test:cover": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "tslint {src,test}/**/*.ts",
    "build": "tsc",
    "prebuild": "rimraf ./dist"
  },
  "keywords": [
    "typescript",
    "network"
  ],
  "author": "Sandro Sobczy≈Ñski <hastoklai@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.5",
    "@types/node": "^8.0.19",
    "coveralls": "^3.1.0",
    "dot-json": "^1.0.3",
    "jest": "^29.3.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^26.5.0",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "@types/readline-sync": "^1.4.3",
    "readline-sync": "^1.4.9"
  }
}
